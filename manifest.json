{
  "name": "w3b1y",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting", "tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["interface.js", "main.js", "img/controller/controller.js", "img/model/model.js", "img/model/status.js", "img/view/view.js", "img/view/view_analysis.js", "img/view/view_result.js", "img/main.js", "heading/controller/controller.js", "heading/model/model.js", "heading/view/view.js", "heading/main.js", "keyword/controller/controller.js", "keyword/view/view.js", "keyword/main.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["static/img/logo.png", "static/img/loading.gif"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "128": "static/img/icon.png"
  }
}