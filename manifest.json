{
  "name": "TETETETETETETETE",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting", "tabs", "storage", "webNavigation"],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["interface.js", "main.js", "img/controller/controller.js", "img/model/model.js", "img/model/status.js", "img/view/view.js", "img/view/view_analysis.js", "img/view/view_result.js", "img/main.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["static/img/logo.png", "static/img/loading.gif"],
      "matches": ["<all_urls>"]
    }
  ]
}